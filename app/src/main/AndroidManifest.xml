<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pe.apiconz.android.khipu" >

    <uses-permission android:name="android.permission.WRITE_SETTINGS" />

    <application
        android:name=".application.Main"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="pe.apiconz.android.khipu.presentation.activities.SplashActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="pe.apiconz.android.khipu.presentation.activities.MainActivity"
            android:label="@string/title_activity_main" >
        </activity>
        <activity
            android:name="pe.apiconz.android.khipu.presentation.activities.CodeActivity"
            android:label="@string/title_activity_code" >
        </activity>
    </application>

</manifest>
